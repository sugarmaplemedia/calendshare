{
	"name": "",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"db:migrate": "drizzle-kit generate:pg",
		"db:push": "drizzle-kit push:pg",
		"db:seed": "vite-node --options.transformMode.ssr='/.*/' src/lib/drizzle/seed.ts",
		"db:reset": "supabase db reset && npm run db:push && npm run db:seed"
	},
	"devDependencies": {
		"@prgm/sveltekit-progress-bar": "^2.0.0",
		"@skeletonlabs/skeleton": "^2.5.1",
		"@skeletonlabs/tw-plugin": "^0.2.4",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/forms": "^0.5.7",
		"@types/node": "20.10.0",
		"autoprefixer": "10.4.16",
		"drizzle-kit": "^0.20.14",
		"postcss": "8.4.31",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "3.3.5",
		"tailwindcss-3d": "^1.0.4",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.0",
		"vite-node": "^1.5.0",
		"vite-plugin-tailwind-purgecss": "0.1.4"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "1.5.3",
		"@supabase/auth-helpers-sveltekit": "^0.12.0",
		"@supabase/auth-ui-shared": "^0.1.8",
		"@supabase/auth-ui-svelte": "^0.2.9",
		"@supabase/ssr": "^0.3.0",
		"@supabase/supabase-js": "^2.39.7",
		"cookie": "^0.6.0",
		"drizzle-orm": "^0.29.4",
		"postgres": "^3.4.3",
		"supabase": "^1.145.4",
		"zod": "^3.22.4"
	}
}
